# Database settings (must match Laravel's database)
DB_HOST=mariadb
DB_PORT=3306
DB_DATABASE=laravel
DB_USERNAME=sail
DB_PASSWORD=password

# Laravel APP_KEY for decrypting credentials
# Copy this from the Laravel .env file
APP_KEY=base64:your-app-key-here

# RabbitMQ settings
RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_QUEUE=security-scans

# Prowler settings
PROWLER_OUTPUT_DIR=/tmp/prowler
